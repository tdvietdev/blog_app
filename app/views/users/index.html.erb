<% provide(:title, "All users") %>
<h1>All users</h1>
<%= form_tag users_path, method: "get", enforce_utf8: false do %>
  <%= text_field_tag :search, params[:search]%>
  <%= submit_tag "Search", :name => nil  %>
<% end %>
<ul class="users">
  <%= render @users %>
</ul>
<%= paginate @users, theme: "twitter-bootstrap-3" %>